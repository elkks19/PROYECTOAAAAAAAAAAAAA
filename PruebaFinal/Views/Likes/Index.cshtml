@model IEnumerable<PruebaFinal.Models.Like>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Usuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Producto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fechaLike)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Usuario.codUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Producto.codProducto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fechaLike)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.codLike">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.codLike">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.codLike">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
